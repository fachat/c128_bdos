 2340 cmp int1+1:bcc div2:bne div3
 2350 cpy int1:bcc div2:bne div3
 2360 div2 sta int2+1:sty int2:inx:bne div1
 2370 div3 dex:bpl div6:ldy abuf:lda int3:ldx int3+1:rts
 2380 div6 lda int1+1:cmp int2+1:bcc div4:bne div5
 2390 lda int1:cmp int2:bcc div4
 2400 div5 lda int1:sec:sbc int2:sta int1:lda int1+1:sbc int2+1:sta int1+1
 2410 sec:.byt $24:div4 clc:rol int3:rol int3+1
 2420 lsr int2+1:ror int2:jmp div3
 2450 ;****
 2490 ; int1 * a,y = a,y
 2500 mult sta int2
 2510 lda #0:sta int3:sta int3+1
 2520 mul1 txa:lsr:tax:ror int2:ora int2:bcs mul2:beq multe
 2530 bcc mul3
 2540 mul2 lda int3:clc:adc int1:sta int3:lda int3+1:adc int1+1:sta int3+1
 2550 mul3 asl int1:rol int1+1:jmp mul1
 2555 multe lda int3:ldx int3+1
 2560 sta int1:stx int1+1:rts
